Cell[TextData[{
 "Thermodynamic data from rescaled metric :\nNote that the ``t\
\[CloseCurlyQuote]\[CloseCurlyQuote] denotes tilded quantities, e.g. qt is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["q", "~"], "."}], TraditionalForm]]],
 " While uHt is the inverse square of the location of the horizon in \
r-coordinates, i.e. uHt=1/",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["rH", "2"], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.5942539577242117`*^9, 3.594253967002379*^9}, {
  3.5942551010037127`*^9, 3.59425512393373*^9}, {3.6490155660102797`*^9, 
  3.6490156487356367`*^9}, {3.6491011689103327`*^9, 3.6491012556079597`*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Tt", "[", 
    RowBox[{"qt_", ",", "m_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"1", "/", 
     RowBox[{"(", 
      RowBox[{"2", " ", "\[Pi]"}], ")"}]}], " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"m", "/", 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"qt", "^", "2"}]}], ")"}]}], ")"}], "^", 
     RowBox[{"(", 
      RowBox[{"1", "/", "4"}], ")"}]}], " ", 
    RowBox[{"Abs", "[", 
     RowBox[{
      RowBox[{"-", "2"}], "+", 
      RowBox[{"qt", "^", "2"}]}], "]"}]}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Mu]t", "[", 
    RowBox[{"qt_", ",", "m_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"Sqrt", "[", "3", "]"}], "/", "2"}], " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"m", "/", 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"qt", "^", "2"}]}], ")"}]}], ")"}], "^", 
     RowBox[{"(", 
      RowBox[{"1", "/", "4"}], ")"}]}], " ", "qt"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"uHt", "[", 
   RowBox[{"qt_", ",", "m_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"qt", "^", "2"}]}], ")"}], "/", "m"}], ")"}], "^", 
   RowBox[{"(", 
    RowBox[{"1", "/", "2"}], ")"}]}]}]}], "Input",
 CellChangeTimes->{{3.5942539692564497`*^9, 3.594254095488097*^9}}],

